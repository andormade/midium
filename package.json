{
  "name": "midium",
  "author": {
    "name": "Andor Polgar",
    "email": "hngrhorace@gmail.com",
    "url": "http://polgarand.org"
  },
  "version": "0.0.7",
  "description": "Web MIDI API wrapper designed for humans and MIDI instruments",
  "keywords": [
    "midi",
    "web",
    "api"
  ],
  "bugs": {
    "url": "https://github.com/midijs/midium/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/midijs/midium.git"
  },
  "main": "dist/index.js",
  "dependencies": {
    "midinette": "^1.0.4",
    "midium-core": "^1.0.15"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "browserify": "^12.0.1",
    "eslint": "^1.10.1"
  },
  "scripts": {
    "build:babel": "babel src -d dist",
    "build:browserify": "mkdir -p browser && browserify dist/index.js --standalone Midium > browser/midium.js",
    "build": "npm run build:babel && npm run build:browserify",
    "eslint": "eslint src/*.js"
  }
}
